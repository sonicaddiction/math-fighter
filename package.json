{
  "name": "personal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "$(npm bin)/flow && react-scripts start",
    "build": "$(npm bin)/flow && react-scripts build",
    "test": "$(npm bin)/flow && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "$(npm bin)/flow",
    "postinstall": "$(npm bin)/flow-typed install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4"
  }
}
